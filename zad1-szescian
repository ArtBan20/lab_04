#include <iostream>
using namespace std;
int szescian(int a) {

    return 6 * (a * a);
}
int objetosc(int a) {
    return a * a * a;
}

int main()
{
    int a;
    cout << "Podaj a: " << endl;
    cin >> a;
    cout << "Pole szescianu wynosi: " << szescian(a) << endl;;
    cout << "Objetosc szcescianu wynosi: " << objetosc(a);
}


